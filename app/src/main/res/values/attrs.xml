<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="allen">
        <attr name="showDividerLine" format="boolean" />
        <attr name="dividerLineWidth" format="float" />
    </declare-styleable>

    <declare-styleable name="DashboardView">
        <attr name="padding" format="dimension" />
        <attr name="progressStrokeWidth" format="dimension" />
        <attr name="android:text" />
        <attr name="unit" format="string" />
        <attr name="android:textSize" />
        <attr name="backgroundColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="startProgressColor" format="color" />
        <attr name="endProgressColor" format="color" />
        <attr name="startNumber" format="integer" />
        <attr name="maxNumber" format="integer" />
        <attr name="progressColor" format="color" />
        <attr name="tikeStrArray" format="reference" />
        <attr name="tikeStrSize" format="dimension" />
        <attr name="tikeStrColor" format="color" />
        <attr name="centerCircleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PowerImageView">
        <attr name="auto_play" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor2" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textType" format="string" />
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
</resources>